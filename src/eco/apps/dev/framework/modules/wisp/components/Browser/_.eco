
std Module
{
    Behavior <- ($package modules polymorphic) NewBehavior
                    ~ name: 'a browser'
                    ~ build;
    
    State :: Classlike
    {
        Instance :: Clonable
        {
            |
                views,
            |

            {
                init => (
                    views <- std util newStack.
                  ^ self
                );

                writeOn: w => w << 'a browser state';
            }
        };
    };

}
