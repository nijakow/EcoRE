
{
    isa std Classlike;

    Instance :: std Clonable
    {
        {
            protected polymorphic <- $package;

            init => (
              ^ self
            );

            newSession => ((polymorphic core world) Session) World: self;

            writeOn: w => w << 'a polymorphic world';
        }
    };

    New => Instance new;
}
