
{
    isa std Classlike;

    Instance :: Clonable
    {
        |
            parent <- No,
            morph
        |

        {
            initWithMorph: m => (
                parent <- No.
                morph  <- m.
              ^ self
            );

            world => (parent) yes: [ :p | ^ p world ] no: [ ^ No ];

            writeOn: w => w << 'a morph node';
        }
    };

    Morph: morph => (Instance clone) initWithMorph: morph;
}
