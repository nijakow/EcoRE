
{
    isa std Classlike;

    Instance :: Clonable
    {
        |
            parent <- No
        |

        {
            initWithParent: p => (
                parent <- Yes: p.
              ^ self
            );

            world => (parent) yes: [ :p | ^ p world ] no: [ ^ No ];

            writeOn: w => w << 'a morph node';
        }
    };

    Parent: parent => (Instance clone) initWithParent: parent;
}
