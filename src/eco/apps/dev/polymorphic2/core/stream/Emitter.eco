
{
    static with inherited _ = std Classlike;

    own Instance = {
        static with inherited _ = std Clonable;

        protected paper;

        with inherited trait = {
            static with inherited _ = std Trait;
            
            initWithPaper: _paper => (
                paper <- _paper.
              ^ self
            );

            emit: kernel bounds: bounds => (
                paper place: kernel bounds: bounds.
              ^ self
            );

            emit: kernel at: point => (
              ^ emit: kernel bounds: (point extent: (kernel minExtent))
            );
            
            writeOn: w => w << 'an emitter';
        };
    };

    Paper: paper => (Instance clone) initWithPaper: paper;
}
