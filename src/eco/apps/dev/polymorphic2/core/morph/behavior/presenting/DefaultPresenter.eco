
{
    isa std Singleton;

    with inherited PresenterTrait <- $package;

    minWidthOfKernel:  kernel => 50;
    minHeightOfKernel: kernel => 10;

    drawOn: stream Kernel: kernel => (
        stream emit: ((polymorphic core objects Text) Caption: 'Morph!').
      ^ self
    );

    writeOn: w => w << 'the default presenter';
}
