
{
    isa std Classlike;

    Instance :: Clonable
    {
        |
            presentations
        |

        {
            init => (
                presentations <- std util newVector.
              ^ self
            );

            renderOn: renderer => (
                presentations do: [ :each | each renderOn: renderer ].
              ^ self
            );

            writeOn: w => w << 'a paper';
        }
    };

    New => Instance new;
}
