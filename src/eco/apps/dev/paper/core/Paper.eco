
{
    static with inherited _ = std Classlike;

    own Instance = {
        static with inherited _ = std Clonable;

        protected program;

        with inherited trait = {
            static with inherited _ = std Trait;
            protected paper = $package;

            init => (
                program <- ((paper core program) PaperProgram) New.
              ^ self
            );

            writeOn: w => w << 'a paper';
        };
    };

    New => Instance new;
}
