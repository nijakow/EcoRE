
{
    static with inherited _ = std Module;
    with app = $;

    own core = (here / 'core' / '_.eco') importIn: self;

    SampleMorph => (
        |
            world <- (core world) NewWorld,
            morph <- (core MorphInstance) Behavior: (core MorphBehavior New)
        |

        (morph structure) parentInfo: ((core structure parent) WorldParent World: world).

        (morph behavior) layouter: (((core layouting) layouters StackLayouter) New).

      ^ morph
    );
}
