
#=globals: {
    char = {
        static with inherited _ = #<:std/Module>;
        
        space     => 32 asChar;
        backspace =>  8 asChar;
        tab       =>  9 asChar;
        newline   => 10 asChar;
        return    => 13 asChar;
        escape    => 27 asChar;
    };

    any = #<:std/bool/false>;

    begin = #<:std/bool/true>;
}
