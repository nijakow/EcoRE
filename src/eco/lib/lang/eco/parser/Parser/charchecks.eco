
#=lang/eco/parser/Parser/CharChecks: {
    static with inherited _ = #<:std/Module>;
    
    IsBin(c) => (
        | rfalse <- [ ^ false ] |

        ([ c >= #\a ] and: [ c <= #\z ]) if: rfalse.
        ([ c >= #\A ] and: [ c <= #\Z ]) if: rfalse.
        ([ c >= #\0 ] and: [ c <= #\9 ]) if: rfalse.
        (c == #\!) if: rfalse.
        (c == #\_) if: rfalse.
        (c == #\?) if: rfalse.

      ^ true
    );
}
