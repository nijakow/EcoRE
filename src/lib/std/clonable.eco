
#=std/Clonable: {
    with inherited parent = #<:std/DefaultBehavior>;
    init          => ^ self;
    clone         => #'ecosphere.ecore.clone'(self);
    new           => clone init;
    new: e        => clone init: e;
    writeOn: writer => writer << 'a clonable';
    isBlock       => false;
    isInt         => false;
    isChar        => false;
    isKey         => false;
    isString      => false;
    isArray       => false;
}
