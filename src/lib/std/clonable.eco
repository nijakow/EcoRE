
#=std/Clonable: {
    with inherited parent = #<:std/DefaultBehavior>;
    init          => ^ self;
    clone         => #'ecosphere.ecore.clone'(self itself);
    new           => clone init;
    writeOn: writer => writer << 'clonable';
    isBlock       => false;
    isInt         => false;
    isChar        => false;
    isKey         => false;
    isString      => false;
    isArray       => false;
}
