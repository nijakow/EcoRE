
#=std/reflect/primitives: {
    static with inherited _ = #<:std/Module>;

    IsInt:   obj => obj isInt;
    IsFloat: obj => obj isFloat;
    IsChar:  obj => obj isChar;

    IsKey:    obj => obj isKey;
    IsString: obj => obj isString;

    IsArray:  obj => obj isArray;

    IsBlock:  obj => obj isBlock;
    IsCode:   obj => obj isCode;
}
