"
    util.eco - The `std util` package of the Ecosphere
"

#=std/util: {
    Pair = {
        first, second;

        with inherited trait = {
            isPair => true;
            first: f second: s => (
                first  <- f.
                second <- s.
              ^ self
            );
        };

        with inherited parent = #<:std/Clonable>;
    }
}
