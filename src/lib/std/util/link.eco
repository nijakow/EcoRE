#=std/util/Link: {
    own next,
    own prev,
    value;

    with inherited trait = {
        init => (next: self prev: self) value: self;
        next: value => ( next <- value. ^ self );
        prev: value => ( prev <- value. ^ self );
        next: nv prev: pv => (next: nv) prev: pv;
        value: nv => ( value <- nv. ^ self );
    };

    with inherited parent = #<:std/Clonable>;
}
