
#=std/util/Queue: {
    private elements;

    with inherited trait = {
        init => (
            elements <- std util newVector.
          ^ self
        );

        isEmpty => elements isEmpty;
        enqueue: e => elements append: e;
        dequeue => elements removeFirst;
        front => elements first;

        writeOn: w => w << 'a queue';
    };

    with inherited parent = #<:std/Clonable>;
}
