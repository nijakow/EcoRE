
#=std/io/StringWriter: {
    private builder;

    with inherited trait = {
        init => (
            builder <- std util newVector.
          ^ self
        );

        char<< c => builder append: c.

        commit => builder asString;
        close => self;

        writeOn: w => w << 'a string writer';

        with inherited parent = #<:std/io/Writer>;
    };

    static with inherited parent = #<:std/Clonable>;
}
