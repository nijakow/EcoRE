"
    reflect.eco - The `std reflect` package of the Ecosphere
"

#=std/reflect: {
    Mirror = {
        reflectee;

        with inherited trait = {
            init: the_reflectee => (
                reflectee <- the_reflectee.
              ^ self
            );
        };

        with inherited parent = #<:std/Clonable>;
    }
}
