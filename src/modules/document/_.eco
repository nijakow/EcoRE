{
    "
        Document Module
    "
    static with inherited _ = std Module;

    own Document = (here / 'Document' / '_.eco') importIn: self;

    own editing = (here / 'editing' / '_.eco') importIn: self;

    Test => (
        |
            d <- Document new,
            c <- d newCaret
        |

        c insertText: '(2 + '.
        c insertReference: 2.
        c insertText: ') * '.
        c insertCharacter: $'3'.
        c insertCharacter: $'.'.
        
      ^ d
    );
}
